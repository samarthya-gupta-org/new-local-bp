{
  "flavor": "vm",
  "metadata": {
    "labels": {}
  },
  "kind": "service",
  "disabled": true,
  "provided": false,
  "version": "0.3",
  "spec": {
    "enable_host_anti_affinity": false,
    "type": "application",
    "release": {
      "strategy": {
        "type": "Recreate"
      },
      "image": "image_url"
    },
    "runtime": {
      "autoscaling": {
        "min": "1",
        "max": "1",
        "cpu_threshold": "50"
      },
      "ports": {
        "http": {
          "port": "8082",
          "protocol": "tcp"
        }
      },
      "size": {
        "cpu": "300m",
        "memory": "2Gi",
        "cpu_limit": "1000m"
      },
      "health_checks": {
        "timeout": "10",
        "period": "32",
        "port": "8080",
        "start_up_time": "20"
      },
      "volumes": {}
    },
    "env": {
      "LOG_LEVEL": "INFO_2"
    }
  }
}